<div class="container-fluid  px-sm-3 px-md-5"
    style="background: #111111; margin-top: 20px; padding-top: 10px; padding-bottom: 10px;">
    <h5 class="m-0 text-center active" style="color: #FFCC00; text-transform: uppercase; font-weight: bold;">LATEST Newses</h5>

</div>







<div class="container-fluid">
    <div class="row" style="margin:0px 70px 0px 70px ;">
        <div class="col-xl-11">

        </div>
        <div class="col-xl-1">
            <div class="admin-action" style="width: 150px; margin-right: 30px;">
                <a href="create_latest_news">Create New</a>
            </div>
        </div>
    </div>

    <% if(latest.length>0){%>
        <%latest.forEach(latest=>{ %>
            <div class="row"
                style="padding: 0px 10px 20px 10px;background-color: whitesmoke; padding: 0px 10px 20px 10px; margin: 40px; height: 200px">
                <div class="col-md-11" style="height: 40px; ">

                    <div class="row">
                        <div class="col-md-4" style="height: 200px; ">
                            <div class="breaking-content-img">
                                <img src="" alt="">
                            </div>

                        </div>
                        <div class="col-md-7" style=" height: 200px; ">
                            <div class="row">
                                <div class="row pt-3 pl-4" >
                                    <p style="float: left; color: #ffcc00; background-color: #111111;font-size: 12px; padding: 5px 5px 5px 5px; font-weight: bold;">
                                        <%= latest.language%>
                                    </p>
                                    <p style="text-align: right; padding: 0px 0px 0px 40px; font-size: 14px; float:right; margin-bottom: 0px; " >
                                        <%= latest.date_of_post%>
                                    </p>
                                </div>
                                <div class="breaking-headline headlines">
                                    <a href="/single_latest/<%= latest.id%>" target="_blank">
                                        <h3>
                                            <%= latest.latest_news_headline%>
                                        </h3>
                                    </a>
                                </div>
                                <div class="slider_details headlines_details" style="text-decoration: none;">
                                    <p>
                                        <%= latest.latest_news_details%>
                                    </p>
                                </div>
                            </div>

                        </div>
                        <div class="col-md-1" style=" height: 200px; ">
                            <div class="row">
                                <div class="ref-link"
                                    style="text-align: center; font-size: 12px; padding: 10px 10px 10px 10px; display: block;">
                                    <p>Link:</p>
                                    <a href="<%= latest.ref_link %>" target="popup"
                                        onclick="window.open('<%= latest.ref_link %>','popup','width=600,height=600'); return false;">Click Here</a>
                                </div>
                            </div>
                            <div class="row">
                                <div class="comments">

                                </div>
                            </div>
                        </div>
                    </div>


                </div>
                <div class="col-md-1" style="height: 200px; ">
                    <div class="row">
                        <div class="admin-action" style="margin-top: 30px;">
                            <a href="/edit_latest/<%=latest.id%>">Edit</a>
                        </div>
                    </div>
                    <div class="row">
                        <div class="admin-action" style="margin-top: 10px;">
                            <a href="/delete_latest/<%=latest.id%>"  onclick="javascript:return confirm('Are you sure! You want to DELETE ?')">Delete</a>
                        </div>
                    </div>
                </div>
            </div>
            <%})%>
                <%}%>
</div>








